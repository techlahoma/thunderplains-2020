<div class="container mx-auto py-8 px-4 hidden">
  <h2 id="schedule">Schedule</h2>

  {% for schedule in schedule %}
  <div class="p-0 sm:p-4">
    <h3 class="bg-teal-dark text-teal-lightest p-4 m-0 text-center">{{ schedule.start_time }}</h3>

    <div class="flex flex-col md:flex-row">
      {% if schedule.full %}
      <div class="m-0 px-0 py-4 sm:px-4">
        <div class="text-xl font-semibold">{{ schedule.full.title }}</div>

        {% if schedule.full.speaker %}
          {% if schedule.full.speaker.twitter %}
          <div><a href="https://www.twitter.com/{{ schedule.full.speaker.twitter }}">{{ schedule.full.speaker.name }}</a></div>
          {% else %}
          <div>{{ schedule.full.speaker.name }}</div>
          {% endif %}
        {% endif %}
      </div>

      {% else %}
      <div class="flex flex-col w-full md:w-1/2 m-0">
        <div class="bg-teal-lighter text-center m-0 p-2">Room 16 (Upstairs)</div>
        <div class="px-0 py-4 sm:px-4">
          <div class="text-xl font-semibold">{{ schedule.track_a.title }}</div>

          {% if schedule.track_a.speaker %}
            {% if schedule.track_a.speaker.twitter %}
            <div><a href="https://www.twitter.com/{{ schedule.track_a.speaker.twitter }}">{{ schedule.track_a.speaker.name }}</a></div>
            {% else %}
            <div>{{ schedule.track_a.speaker.name }}</div>
            {% endif %}
          {% endif %}
        </div>
      </div>
      {% endif %}

      {% if schedule.track_b %}
      <div class="flex flex-col w-full md:w-1/2 m-0">
        <div class="bg-yellow-light text-center m-0 p-2">Room 17 (Upstairs)</div>
        <div class="px-0 py-4 sm:px-4">
          <div class="text-xl font-semibold">{{ schedule.track_b.title }}</div>

          {% if schedule.track_b.speaker %}
            {% if schedule.track_b.speaker.twitter %}
            <div><a href="https://www.twitter.com/{{ schedule.track_b.speaker.twitter }}">{{ schedule.track_b.speaker.name }}</a></div>
            {% else %}
            <div>{{ schedule.track_b.speaker.name }}</div>
            {% endif %}
          {% endif %}
        </div>
      </div>
      {% endif %}
    </div>
  </div>
  {% endfor %}
</div>


<div class="container mx-auto py-8 px-4 border-b">

  {% for sponsorLevel in sponsors %}

  <div class="m-4">
    <h3 class="text-center">{{ sponsorLevel.title }}</h3>
    <div class="container flex flex-row flex-wrap items-center justify-between content-center">
    {% for sponsor in sponsorLevel.companies %}
      <div class="flex-1 border-4 border-{{ sponsorLevel.tailwind_color }} text-center w-full md:w-1/2 m-4 p-4" style="min-width: {{ sponsorLevel.img_width }}px">
        <a href="{{ sponsor.a_href }}" target="_blank">
          <img src="/images/sponsor-logos/{{ sponsor.img_src }}" width="{{ sponsorLevel.img_width }}" /><br />{{ sponsor.name }}
        </a>
      </div>
    {% endfor %}
    </div>
  </div>
  {% endfor %}

  <div class="max-w-screen-xl mx-auto text-center py-12 px-4 sm:px-6 lg:py-16 lg:px-8">
    <h2 class="text-3xl leading-9 font-extrabold tracking-tight text-gray-900 sm:text-4xl sm:leading-10">
      Want To Sponsor ThunderPlains?
    </h2>
    <div class="mt-8 flex justify-center">
      <div class="inline-flex rounded-md shadow">
        <a href="/images/2020SponsorshipProspectus.pdf" class="inline-flex items-center justify-center px-5 py-3 border border-transparent text-base leading-6 font-medium rounded-md text-white bg-indigo-600 hover:bg-indigo-500 focus:outline-none focus:shadow-outline transition duration-150 ease-in-out">
          Learn more
        </a>
      </div>
    </div>
  </div>

</div>

